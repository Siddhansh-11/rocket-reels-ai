        "NOTION_API_KEY": "your-notion-api-key",
        "NOTION_DATABASE_ID": "your-notion-database-id",
        "LANGCHAIN_API_KEY": "your-langchain-api-key",
        "ELEVENLABS_API_KEY": "your-elevenlabs-api-key",
        "PEXELS_API_KEY": "your-pexels-api-key",
